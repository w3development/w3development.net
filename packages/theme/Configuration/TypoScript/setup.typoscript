@import 'EXT:fluid_styled_content/Configuration/TypoScript/setup.typoscript'
@import 'EXT:fluid_styled_content/Configuration/TypoScript/Styling/setup.typoscript'
// @import 'EXT:form/Configuration/TypoScript/setup.typoscript'

// @import 'EXT:theme/Configuration/TypoScript/Main/Extensions/seo.typoscript'
@import 'EXT:theme/Configuration/TypoScript/Main/Navigation/topnavigation.typoscript'
@import 'EXT:theme/Configuration/TypoScript/Main/Navigation/languageswitcher.typoscript'
@import 'EXT:theme/Configuration/TypoScript/Main/Navigation/alternatelanguage.typoscript'
// @import 'EXT:theme/Configuration/TypoScript/Main/Navigation/getcontent.typoscript'
// @import 'EXT:theme/Configuration/TypoScript/Main/Configurations/development.typoscript'

// default/general configuration (will add 'children_<colPos>' variable to processedData for each colPos in container
@import 'EXT:theme/Configuration/TypoScript/Main/ContentElement'

// dynamic content element base on colPos.
@import 'EXT:theme/Configuration/TypoScript/Helper/DynamicContent.typoscript'

page = PAGE
page {
  # Favicon Configurations
  shortcutIcon = EXT:theme/Resources/Public/Images/Favicon/favicon-16x16.png

  meta {
    X-UA-Compatible = IE=edge
    X-UA-Compatible.attribute = http-equiv
    robots = index, follow
    keywords = {$page.meta.keywords}
    keywords.override.field = keywords
    description = {$page.meta.description}
    description.override.field = description
    author = w3development.net
    msapplication-TileColor = #da532c
    theme-color = #ffffff
    viewport = width=device-width, initial-scale=1.0, minimum-scale=1.0
    msvalidate\.01 = F4570B6CF9303302640959810103F14C
    yandex-verification=1ae4de3597e2db92
    google-site-verification=RPR50Qap0Tyvw5081Ff25zG9r-4-nYi4CFaENRFytr4
  }

  includeCSS{
    inline = EXT:theme/Resources/Public/Stylesheets/theme.css
    inline {
      inline = 1
      forceOnTop = 1
      media = all
    }

    fontawesome = EXT:theme/Resources/Public/Stylesheets/all.css
  }

  includeJSFooterlibs {
    jQuery = https://code.jquery.com/jquery-3.7.1.js
    jQuery {
      external = 1
      integrity = sha256-eKhayi8LEQwp4NKxN+CfCh+3qOVUtJn3QNZ0TciWLP4=
      disableCompression = 1
      excludeFromConcatenation = 1
      crossorigin = anonymous
    }
  }

  headerData{
    86199 = FLUIDTEMPLATE
    86199.file = EXT:theme/Resources/Private/Partials/Page/HeadTag/Icons.html

    86399 = FLUIDTEMPLATE
    86399.file = EXT:theme/Resources/Private/Partials/Page/HeadTag/Schema.html

    86999 = FLUIDTEMPLATE
    86999.file = EXT:theme/Resources/Private/Partials/Page/HeadTag/GTM.html
  }

	10 = FLUIDTEMPLATE
	10 {
		templateName = TEXT
		templateName {
			cObject = TEXT
			cObject {
				data = pagelayout
				required = 1
				case = uppercamelcase
				split {
					token = pagets__
					cObjNum = 1
					1.current = 1
				}
			}
		  ifEmpty = Default
		}

		templateRootPaths {
			0 = EXT:theme/Resources/Private/Templates/Page/
		}
		partialRootPaths {
			0 = EXT:theme/Resources/Private/Partials/Page/
		}
		layoutRootPaths {
			0 = EXT:theme/Resources/Private/Layouts/Page/
		}
	}
}
