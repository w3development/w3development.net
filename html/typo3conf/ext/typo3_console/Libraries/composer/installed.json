            "name": "helhum/typo3-console",
            "version": "v6.7.7",
            "version_normalized": "6.7.7.0",
            "dist": {
                "type": "path",
                "url": "/home/runner/work/TYPO3-Console/TYPO3-Console",
                "reference": "4b7123a8198978b46bd2370c685d44432242a2b3"
            },
            "require": {
                "helhum/config-loader": ">=0.9 <0.13",
