{
	"intelephense.files.exclude": [
		"**/.git/**",
		"**/.svn/**",
		"**/.hg/**",
		"**/CVS/**",
		"**/.DS_Store/**",
		"**/node_modules/**",
		"**/bower_components/**",
		"**/vendor/**/{Tests,tests}/**",
		"**/.history/**",
		"**/vendor/**/vendor/**",
		"**/typo3_src-10.4.37/**",
		"**/typo3_src/**",
	],
	"sqltools.connections": [
		{
			"mysqlOptions": {
				"authProtocol": "default",
				"enableSsl": "Disabled"
			},
			"previewLimit": 50,
			"server": "w3development.local",
			"port": 33061,
			"driver": "MySQL",
			"name": "w3development_docker",
			"database": "db",
			"username": "typo3"
		}
	]
}
